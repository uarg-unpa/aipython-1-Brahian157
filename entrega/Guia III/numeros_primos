numero = int(input("Introduce un número entero: "))

if numero <= 1:
    es_primo = False
else:
    es_primo = True
    for i in range(2, numero):
        if numero % i == 0:
            es_primo = False

if es_primo:
    print(f"{numero} es un número primo.")
else:
    print(f"{numero} no es un número primo.")